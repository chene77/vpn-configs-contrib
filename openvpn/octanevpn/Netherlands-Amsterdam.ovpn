# host/port of vpn server
remote gw1.ams1.octanevpn.com 8080
remote gw1.ams2.octanevpn.com 8080
remote gw1.ams3.octanevpn.com 8080
remote gw1.ams4.octanevpn.com 8080
remote gw2.ams3.octanevpn.com 8080

auth-user-pass /config/openvpn-credentials.txt
client
tls-cipher "DEFAULT:@SECLEVEL=0"
redirect-gateway
remote-cert-tls server
data-ciphers AES-256-CBC
data-ciphers-fallback AES-256-CBC
proto udp
dev tun
nobind
# CA certificate used for server cert validation
ca /etc/openvpn/octanevpn/ca.crt